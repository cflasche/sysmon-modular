<Sysmon schemaversion="4.30">
  <EventFiltering>
    <RuleGroup name="" groupRelation="or">
      <NetworkConnect onmatch="exclude">
        <Image condition="end with">AppData\Local\Microsoft\Teams\current\Teams.exe</Image>        <!--Microsoft: Teams-->
      </NetworkConnect>
    </RuleGroup>
  </EventFiltering>
</Sysmon>